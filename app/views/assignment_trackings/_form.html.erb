<%= simple_form_for(@assignment_tracking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :assignment_id %>
    <%= f.input :user_id %>
    <%= f.input :topic %>
    <%= f.input :solution %>
    <%= f.input :git_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
