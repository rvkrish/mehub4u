<p id="notice"><%= notice %></p>

<h1>Listing Assignment Trackings</h1>

<table>
  <thead>
    <tr>
      <th>Assignment</th>
      <th>User</th>
      <th>Topic</th>
      <th>Solution</th>
      <th>Git url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assignment_trackings.each do |assignment_tracking| %>
      <tr>
        <td><%= assignment_tracking.assignment_id %></td>
        <td><%= assignment_tracking.user_id %></td>
        <td><%= assignment_tracking.topic %></td>
        <td><%= assignment_tracking.solution %></td>
        <td><%= assignment_tracking.git_url %></td>
        <td><%= link_to 'Show', assignment_tracking %></td>
        <td><%= link_to 'Edit', edit_assignment_tracking_path(assignment_tracking) %></td>
        <td><%= link_to 'Destroy', assignment_tracking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assignment tracking', new_assignment_tracking_path %>
