<p id="notice"><%= notice %></p>

<div class = "col-sm-4 col-sm-offset-4">
   <div class="panel panel-default manage-tabs manage-students-panel">
     <div class="panel-heading default-color"> Faculty Information</div>
      <div class = "panal-body" style = "margin-left:4%">
        <p id="notice"><%= notice %></p>
        <div class="row form-group">
          <div class="col-sm-6">
            <strong>Url:</strong>
          </div>
          <div class="col-sm-6">
            <%= @video.url %>
          </div>
        </div>
         <div class="row form-group">
          <div class="col-sm-6">
            <strong>Type:</strong>
          </div>
          <div class="col-sm-6">
           <%= @video.video_type %>
          </div>
        </div>
        
        <div class="row form-group">
          <div class="col-sm-6 col-sm-offset-6">
            <%= link_to 'Edit', edit_video_path(@video),  class: "btn btn-default"%>
            <%= link_to 'Back', admin_index_path,  class: "btn btn-default" %>
          </div>
        </div>
        
        
      </div>
    </div>
  
</div>


