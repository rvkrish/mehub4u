<p id="notice"><%= notice %></p>
<!-- 
<h1>Listing Project Tackings</h1>
 -->

<div class="container">
  <div class="col-md-2">
    <%= link_to 'New Project tacking', new_project_tacking_path %> | <%= link_to "Add a new key", '#', :"data-toggle"  => "modal", :"data-target" => "#myModal" %>
    
  </div>

  <div class="col-md-8">
    <div class="table-responsive table-hover">
      <table class="table">
        <thead>
          <tr>
            <th>Project</th>
            <th>Git url</th>
            <th>Key</th>
            <th>Value</th>
            <th>Completed</th>
            <th>Remaining</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @tracking.each do |project_tacking| %>
            <tr>
              <td><%= project_tacking.project_id %></td>
              <td><%= project_tacking.git_url %></td>
              <td><%= project_tacking.key %></td>
              <td><%= project_tacking.value %></td>
              <td><%= project_tacking.completed %></td>
              <td><%= project_tacking.remaining %></td>
              <td><%= link_to 'Show', project_tacking %></td>
              <td><%= link_to 'Edit', edit_project_tacking_path(project_tacking) %></td>
              <td><%= link_to 'Destroy', project_tacking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <div class="col-md-2">
    
  </div>
</div>





