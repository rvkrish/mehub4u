<div class = "col-sm-4 col-sm-offset-4">
   <div class="panel panel-default manage-tabs manage-students-panel">
     <div class="panel-heading default-color"> Course Information</div>
      <div class = "panal-body" style = "margin-left:4%">
        <p id="notice"><%= notice %></p>
        <div class="row form-group">
          <div class="col-sm-6">
            <strong>Course name:</strong>
          </div>
          <div class="col-sm-6">
            <%= @course.course_name %>
          </div>
        </div>
         <div class="row form-group">
          <div class="col-sm-6">
            <strong>Course description:</strong>
          </div>
          <div class="col-sm-6">
            <%= @course.course_description %>
          </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-6 col-sm-offset-6">
            <%= link_to 'Edit', edit_course_path(@course),  class: "btn btn-default"%>
            <%= link_to 'Back', admin_index_path,  class: "btn btn-default" %>
          </div>
        </div>
        
        
      </div>
    </div>
  
</div>